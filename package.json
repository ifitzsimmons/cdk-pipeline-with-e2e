{
  "name": "thumbnail-cdk",
  "version": "0.1.0",
  "bin": {
    "thumbnail-cdk": "bin/thumbnail-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "watch": "tsc -w",
    "postinstall": "if test \"$CODEBUILD_CI\" != true ; then npm run install-husky ; fi",
    "install-husky": "npx husky install && npx husky add .husky/pre-commit \"npx lint-staged\"",
    "test": "jest",
    "test:lambda": "python3 -m tox -c src/tox.ini",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "^29.5.0",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "aws-cdk": "^2.72.1",
    "aws-cdk-lib": "^2.72.1",
    "aws-sdk": "^2.1084.0",
    "constructs": "^10.1.299",
    "esbuild": "^0.17.15",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "winston": "^3.6.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint"
    ]
  }
}
